{
    "name": "mnapoli/php-static-analyzer",
    "description": "PHP static analyzer",
    "keywords": ["php", "analyzer", "type", "inference"],
    "license": "MIT",
    "type": "library",
    "autoload": {
        "psr-4": {
            "PhpAnalyzer\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "PhpAnalyzer\\Test\\": "tests/"
        }
    },
    "require": {
        "php": ">=5.5",
        "nikic/php-parser": "~1.2",
        "symfony/finder": "~2.6",
        "mnapoli/silly": "~1.0",
        "phpdocumentor/reflection-docblock": "~2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.5"
    }
}
